<VBox alignment="CENTER" prefHeight="498.0" prefWidth="568.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.smuliknulik.HelloController">
