            <Button fx:id="stop" mnemonicParsing="false" text="Stop" GridPane.columnIndex="3" GridPane.rowIndex="1" />
            <Button fx:id="clean" mnemonicParsing="false" text="Clear" GridPane.columnIndex="4" GridPane.rowIndex="1" />
            <TextField fx:id="timer" GridPane.rowIndex="1" />
